  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.0",
    "chromeless": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "nodemon": "^1.12.1"
  }
}
